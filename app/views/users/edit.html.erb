<%= form_with model:@user, local:true do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  
  <%= f.label :email %>
  <%= f.text_field :email %>
  
  <%= f.label :profile %>
  <%= f.text_field :profile %>
  
  <%= f.label :profile_image %>
  <%= f.text_field :profile_image %>
  
  <%= f.submit %>

<% end %>